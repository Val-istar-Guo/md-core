{
  "name": "md-core",
  "version": "2.0.0-alpha.2",
  "description": "markdown parse core",
  "main": "dist/bundle.js",
  "scripts": {
    "clean": "rm -rf ./dist/*",
    "build": "npm run clean & rollup -c",
    "test": "ava",
    "prepublishOnly": "npm test; npm run build",
    "precommit": "npm test",
    "premili:upgrade": "npm install mili@latest -D",
    "mili:upgrade": "mili upgrade"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Val-istar-Guo/md-core.git"
  },
  "keywords": [
    "markdown",
    "md",
    "md-core"
  ],
  "author": "Val.istar.Guo <Val.istar.Guo@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Val-istar-Guo/md-core/issues"
  },
  "homepage": "https://github.com/Val-istar-Guo/md-core#readme",
  "engines": {
    "node": ">=8.9.4"
  },
  "mili": {
    "type": "component",
    "encoding": "utf8"
  },
  "devDependencies": {
    "ava": "^1.0.0-beta.4",
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "husky": "^0.14.3",
    "mili": "0.0.4",
    "rollup": "^0.58.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0"
  },
  "dependencies": {},
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "sources": [
      "src/**/*.js"
    ],
    "failFast": true,
    "require": [
      "babel-register"
    ]
  }
}
